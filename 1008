var input = require('fs').readFileSync('/dev/stdin', 'utf8');

var varlores = input.split('\n');

var num = parseInt(varlores.shift());
var horasTrab = parseInt(varlores.shift());
var salario = parseFloat(varlores.shift());

var total = (horasTrab*salario).toFixed(2);

console.log(`NUMBER = ${num}`);
console.log(`SALARY = U$ ${total}`);
